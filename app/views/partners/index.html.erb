<h1>Listing Partners</h1>
<%= link_to "Welcome", controller: "welcome" %>
<%= link_to 'New Partner', new_partner_path %>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
 
  <% @partners.each do |partner| %>
    <tr>
      <td><%= partner.name_first %></td>
      <td><%= partner.name_last %></td>
      <td><%= partner.email %></td>
      <td><%= link_to image_tag('show.png'), partner %></td>
      <td><%= link_to image_tag('edit.png'), edit_partner_path(partner) %></td>
      <td><%= link_to image_tag('destroy.png'), partner_path(partner), method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>