<h1>New Partner</h1>
<%= form_for :partner , url: partners_path do |f| %>
  <p>
    <%= f.label :name_first %><br>
    <%= f.text_field :name_first %>
  </p>
 
  <p>
    <%= f.label :name_last %><br>
    <%= f.text_field :name_last %>
  </p>
  <p>
    <%= f.label :email %><br>
    <%= f.email_field :email %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
<%= link_to 'Back', partners_path %>