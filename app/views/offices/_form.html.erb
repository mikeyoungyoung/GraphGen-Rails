<%= form_for([@partner, @partner.offices.build]) do |f| %>
  <p>
    <%= f.label :street %><br />
    <%= f.text_field :street %>
  </p>
    <p>
    <%= f.label :number %><br />
    <%= f.number_field :number %>
  </p>
  <p>
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </p>
  <p>
    <%= f.label :postal_code %><br />
    <%= f.text_field :postal_code %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
